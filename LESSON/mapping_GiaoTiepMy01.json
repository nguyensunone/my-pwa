    {
        "text": "Yes, it's free.",
        "file": "audio_GiaoTiepMy01/audio_GiaoTiepMy01_42_col3.mp3"
    },
    {
        "text": "When does the train arrive?",
        "file": "audio_GiaoTiepMy01/audio_GiaoTiepMy01_43_col0.mp3"
